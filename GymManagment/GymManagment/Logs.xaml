<Window x:Class="GymManagment.Receipt"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GymManagment"
        mc:Ignorable="d"
        Title="Receipt" Height="302" Width="468">
    <Grid>
        <ListView HorizontalAlignment="Left" Height="231" Margin="10,32,0,0" VerticalAlignment="Top" Width="355">
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="Date" Width="100" DisplayMemberBinding="{Binding Name}" />
                    <GridViewColumn Header="Employee Id" Width="100" DisplayMemberBinding="{Binding Age}" />
                    <GridViewColumn Header="Function" Width="150" DisplayMemberBinding="{Binding Mail}" />
                </GridView>
            </ListView.View>
        </ListView>
        <Label Content="Logs" HorizontalAlignment="Left" Margin="10,0,0,0" VerticalAlignment="Top" FontSize="18" FontWeight="Bold"/>
        <Button Content="Print" HorizontalAlignment="Left" Margin="377,59,0,0" VerticalAlignment="Top" Width="75"/>

    </Grid>
</Window>
